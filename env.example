# Environment variables for python-geoaddress
# Copy this file to .env and fill in your values

# =============================================================================
# Address backends - Free providers (no API key required)
# =============================================================================

# Nominatim (OpenStreetMap) - FREE
NOMINATIM_USER_AGENT=my-app/1.0
NOMINATIM_BASE_URL=https://nominatim.openstreetmap.org

# Photon (Komoot/OSM) - FREE
PHOTON_BASE_URL=https://photon.komoot.io

# =============================================================================
# Address backends - Paid/API key required
# =============================================================================

# LocationIQ
LOCATIONIQ_API_KEY=
LOCATIONIQ_BASE_URL=https://api.locationiq.com/v1

# OpenCage
OPENCAGE_API_KEY=
OPENCAGE_BASE_URL=https://api.opencagedata.com/geocode/v1

# Geocode Earth
GEOCODE_EARTH_API_KEY=
GEOCODE_EARTH_BASE_URL=https://api.geocode.earth/v1

# Geoapify
GEOAPIFY_API_KEY=
GEOAPIFY_BASE_URL=https://api.geoapify.com/v1

# Maps.co
MAPS_CO_API_KEY=
MAPS_CO_BASE_URL=https://geocode.maps.co

# Google Maps
GOOGLE_MAPS_API_KEY=

# Mapbox
MAPBOX_ACCESS_TOKEN=

# HERE
HERE_APP_ID=
HERE_APP_CODE=

